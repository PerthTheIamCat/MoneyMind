{
  "expo": {
    "name": "MoneyMind",
    "slug": "MoneyMind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/LOGO.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MoneyMind",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/LOGO.png",
        "backgroundColor": "#F2F2F2"
      },
      "package": "com.anonymous.MoneyMind",
      "googleServicesFile": "./app/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logos/LOGO.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9ac45831-24b1-43eb-978c-32089abf82d4"
      }
    }
  }
}
